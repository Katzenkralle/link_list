    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ll_web/manage.py",
            "args": [
                "runserver"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {"SECRET_KEY": "devsecretkey0346541",
                    "ALLOWED_HOSTS": "127.0.0.1;",
                    "CSRF_TRUSTED_ORIGINS": "http://127.0.0.1",
                    "RUN_IN_DEBUG": "True"}
        },
        {
            "name": "Npm dev",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/ll_web/frontend",
            "command": "npm run dev",
            "request": "launch"
        },
        {
            "name": "Migrate Changes",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/ll_web",
            "program": "${workspaceFolder}/ll_web/migrate_changes.py",
            "args": [""],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {"SECRET_KEY": "devsecretkey0346541",
                    "ALLOWED_HOSTS": "127.0.0.1;",
                    "CSRF_TRUSTED_ORIGINS": "http://127.0.0.1",
                    "RUN_IN_DEBUG": "True"}
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
    ]
    //npm run dev in frontend   
}